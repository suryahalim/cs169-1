<h1>New Restaurant</h1>

<%= form_for @restaurant do |f| %>
<p>
    <%= f.label :address %>
    <%= f.text_field :address %>
</p>
<p>
	<%= f.label :hours %>
    <%= f.text_field :hours %>
</p>
	<%= f.hidden_field :user_id, :value => current_user.id %>
    <%= f.submit "Create restaurant" %>
<% end %>


<%= link_to 'Back', restaurants_path %>
